var myApp = angular.module('myApp', []);

function MyCtrl($scope) {
		$scope.thresLambdaQT = 9;
    $scope.thresMarathonQT = 9;
    $scope.items = [
        {
        name: 'Misko',
        lambdaQueueTime: 5, 
        marathonQueueTime: 10},
        {
        name: 'Igor',
        lambdaQueueTime: 10, 
        marathonQueueTime: 3},
        {
        name: 'Vojta',
        lambdaQueueTime: 2, 
        marathonQueueTime: 7}

    ];
}
